<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:t="sap.ui.table" xmlns:u="sap.ui.unified"
	controllerName="ProjectYPP11BizIT.ProjectYPP11BizIT.controller.Homepage" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Title" showHeader="false" class="css">
				<content>
					<u:FileUploader id="fileUploader" class="form" name="myFileUpload" change="onpresschange" tooltip="Upload your file"
						icon="sap-icon://search"/>
					<Button text="Upload File" press="handleUploadPress" class="myCustomButton form" icon="sap-icon://arrow-bottom"/>
					<t:Table rows="{FileModel>/lines}" selectionMode="MultiToggle" visibleRowCount="7" paste="onPaste" ariaLabelledBy="title">
						<t:columns>
							<t:Column>
								<Label class="label"/>
								<t:template>
									<Text text="{FileModel>header1}" class="sapUiSmallMargin sapThemeHighlight-asColor myCustomText"/>
								</t:template>
							</t:Column>
							<t:Column>
								<Label class="label"/>
								<t:template>
									<Text text="{FileModel>header2}" class="sapUiSmallMargin sapThemeHighlight-asColor myCustomText"/>
								</t:template>
							</t:Column>
							<t:Column>
								<Label class="label"/>
								<t:template>
									<Text text="{FileModel>header3}" class="sapUiSmallMargin sapThemeHighlight-asColor myCustomText"/>
								</t:template>
							</t:Column>
							<t:Column>
								<Label class="label"/>
								<t:template>
									<Text text="{FileModel>header4}" class="sapUiSmallMargin sapThemeHighlight-asColor myCustomText"/>
								</t:template>
							</t:Column>
							<t:Column>
								<Label class="label"/>
								<t:template>
									<Text text="{FileModel>header5}" class="sapUiSmallMargin sapThemeHighlight-asColor myCustomText"/>
								</t:template>
							</t:Column>
							<t:Column>
								<Label class="label"/>
								<t:template>
									<Text text="{FileModel>header6}" class="sapUiSmallMargin sapThemeHighlight-asColor myCustomText"/>
								</t:template>
							</t:Column>
							<t:Column>
								<Label class="label"/>
								<t:template>
									<Text text="{FileModel>header7}" class="sapUiSmallMargin sapThemeHighlight-asColor myCustomText"/>
								</t:template>
							</t:Column>
							<t:Column>
								<Label class="label"/>
								<t:template>
									<Text text="{FileModel>header8}" class="sapUiSmallMargin sapThemeHighlight-asColor myCustomText"/>
								</t:template>
							</t:Column>
						</t:columns>
					</t:Table>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>