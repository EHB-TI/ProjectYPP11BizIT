<mvc:View
 
xmlns="sap.m"
xmlns:core="sap.ui.core"
xmlns:mvc="sap.ui.core.mvc"
xmlns:t="sap.ui.table"
xmlns:u="sap.ui.unified"
xmlns:l="sap.ui.layout"
xmlns:html="http://www.w3.org/1999/xhtml"
    controllerName="ProjectYPP11BizIT.ProjectYPP11BizIT.controller.App" displayBlock="true" id="collection0">
    <Shell id="shell">
        <App id="app">
            <pages>
                <Page id="page" title="{i18n>title}">
                    <content>
                        <u:FileUploader id="fileUploader" class="form" name="myFileUpload" change="onpresschange" tooltip="Upload your file"
                            icon="sap-icon://search" uploadUrl="/sap/opu/odata/sap/YPP11_2_SRV"/>
                        <Button text="Upload File" press="handleUploadPress" class="myCustomButton form" icon="sap-icon://arrow-bottom"/>
                        <Table noDataText="Drop column list items here and columns in the area above" id="table0">
                            <items>
                                <ColumnListItem type="Active" id="item0">
                                    <cells>
                                        <Text text="Row 1 Cell 1" id="text2"/>
                                        <Text text="Row 1 Cell 2" id="text3"/>
                                        <Text text="Row 1 Cell 3" id="text4"/></cells>
                                </ColumnListItem>
                            </items>
                            <columns>
                                <Column id="column0">
                                    <header>
                                        <Label text="Header 1" id="label0"/>
                                    </header>
                                </Column>
                                <Column id="column1">
                                    <header>
                                        <Label text="Header 2" id="label1"/>
                                    </header>
                                </Column>
                                <Column id="column2">
                                    <header>
                                        <Label text="Header 3" id="label2"/>
                                    </header>
                                </Column>
                            </columns>
                        </Table>
                        <PagingButton count="5" id="button2"/>
                    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>